type User {
  id: ID! @id
  name: String!
  email: String! @unique  
  createdAt: DateTime! @createdAt
  description: String
  password: String!
  usertype: String
  isAdmin: Boolean
  lastName: String
  firstName: String
  updatedAt: DateTime @updatedAt
  mobile: String
  profile_image: String
  approved: Boolean
  refferalBonus: String
  profession: String
}


